{
  "name": "github-mcp-custom",
  "version": "1.0.21",
  "description": "Cross-platform NPX-wrappable GitHub MCP Server",
  "main": "bin/index.js",
  "bin": {
    "github-mcp-custom": "bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:windows": "scripts\\\\build.bat",
    "build:linux": "GOOS=linux GOARCH=amd64 go build -ldflags \"-s -w\" -o dist/github-mcp-server-linux .",
    "build:macos": "GOOS=darwin GOARCH=amd64 go build -ldflags \"-s -w\" -o dist/github-mcp-server-macos .",
    "build:win": "GOOS=windows GOARCH=amd64 go build -ldflags \"-s -w\" -o dist/github-mcp-server.exe .",
    "test": "go test -v ./...",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "github",
    "mcp",
    "cli",
    "cross-platform",
    "npx",
    "claude-desktop"
  ],
  "author": "idletoaster",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idletoaster/github-mcp-custom.git"
  },
  "homepage": "https://github.com/idletoaster/github-mcp-custom#readme",
  "bugs": {
    "url": "https://github.com/idletoaster/github-mcp-custom/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
